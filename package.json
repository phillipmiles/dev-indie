{
  "name": "dev-indie",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "_env:dev": "node scripts/setBuildEnv.js development",
    "_env:staging": "node scripts/setBuildEnv.js staging",
    "_env:prod": "node scripts/setBuildEnv.js production",
    "build:dev": "npm run _env:dev && parcel src/index.html --out-dir build",
    "build:staging": "npm run _env:staging && parcel build src/index.html --out-dir build",
    "build:prod": "npm run _env:prod && parcel build src/index.html --out-dir build",
    "dev": "npm run build:dev",
    "test": "jest",
    "test:watch": "jest --watch",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "deploy:staging": "npm run build:staging && firebase deploy -P staging",
    "deploy:prod": "npm run build:prod && firebase use prod && firebase deploy -P prod"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/phillipmiles/dev-indie.git"
  },
  "keywords": [
    "dev",
